{
  "version": 2,
  "projectId": "prj_wNS42qz4RK46r2xNldigbMkL8aCi",
  "orgId": "unite-group",
  "buildCommand": "cd frontend-new && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "frontend-new/dist",
  "framework": "vite",
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "VITE_API_URL": "/api"
    }
  }
}